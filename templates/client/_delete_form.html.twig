 <div class="col-xs-12 col-sm-6">
    <form method="post" action="{{ path('client_delete', {'id': client.id}) }}" onsubmit="return confirm('Are you sure you want to delete this account?');">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ client.id) }}">
        <button class="btn  gradient secondary waves-effect waves-light">Delete</button>
    </form>
</div>
